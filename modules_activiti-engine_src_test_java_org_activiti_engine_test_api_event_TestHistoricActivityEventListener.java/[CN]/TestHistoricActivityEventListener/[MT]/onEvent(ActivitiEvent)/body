{
  if (event.getType().equals(ActivitiEventType.HISTORIC_PROCESS_INSTANCE_CREATED) || event.getType().equals(ActivitiEventType.HISTORIC_PROCESS_INSTANCE_ENDED) || event.getType().equals(ActivitiEventType.HISTORIC_ACTIVITY_INSTANCE_CREATED)|| event.getType().equals(ActivitiEventType.HISTORIC_ACTIVITY_INSTANCE_ENDED)) {
    eventsReceived.add(event);
  }
}
