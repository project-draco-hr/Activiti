{
  Paint originalPaint=g.getPaint();
  g.setPaint(EVENT_COLOR);
  Ellipse2D circle=new Ellipse2D.Double(graphicInfo.getX(),graphicInfo.getY(),graphicInfo.getWidth(),graphicInfo.getHeight());
  g.fill(circle);
  g.setPaint(EVENT_BORDER_COLOR);
  g.draw(circle);
  g.setPaint(originalPaint);
  if (image != null) {
    g.drawImage(image,(int)(graphicInfo.getX() + (graphicInfo.getWidth() / 4)),(int)(graphicInfo.getY() + (graphicInfo.getHeight() / 4)),(int)(image.getWidth() / scaleFactor),(int)(image.getHeight() / scaleFactor),null);
  }
}
