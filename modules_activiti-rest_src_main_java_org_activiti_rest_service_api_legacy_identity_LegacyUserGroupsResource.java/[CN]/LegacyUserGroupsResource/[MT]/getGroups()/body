{
  if (authenticate() == false)   return null;
  String userId=(String)getRequest().getAttributes().get("userId");
  if (userId == null) {
    throw new ActivitiIllegalArgumentException("No userId provided");
  }
  DataResponse dataResponse=new LegacyUserGroupsPaginateList().paginateList(getQuery(),ActivitiUtil.getIdentityService().createGroupQuery().groupMember(userId),"id",properties);
  return dataResponse;
}
