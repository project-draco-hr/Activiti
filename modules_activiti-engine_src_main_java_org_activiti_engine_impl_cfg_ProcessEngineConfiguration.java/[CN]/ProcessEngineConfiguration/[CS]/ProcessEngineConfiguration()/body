  jobExecutor=new JobExecutor();
  jobExecutorActivate=true;
  databaseType="h2";
  dbSchemaStrategy=DbSchemaStrategy.CHECK_VERSION;
  idGenerator=new DbIdGenerator();
  idBlockSize=100;
  dataSource=null;
  transactionsExternallyManaged=false;
  jdbcDriver=DEFAULT_JDBC_DRIVER;
