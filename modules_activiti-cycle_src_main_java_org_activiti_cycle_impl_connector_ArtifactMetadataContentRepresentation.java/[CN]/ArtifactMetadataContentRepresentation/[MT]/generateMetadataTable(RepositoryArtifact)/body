{
  StringBuilder sb=new StringBuilder();
  sb.append("<h2>Metadata</h2>");
  sb.append("<table>");
  for (  Entry<String,String> entry : artifact.getMetadata().getAsStringMap().entrySet()) {
    sb.append("<tr>");
    sb.append("<td><strong>");
    sb.append(entry.getKey());
    sb.append("</strong></td>");
    sb.append("<td>");
    sb.append(entry.getValue());
    sb.append("</td>");
    sb.append("</tr>");
  }
  sb.append("</table>");
  return sb.toString();
}
