{
  ScopedAssociation scopedAssociation=getScopedAssociation();
  if (log.isLoggable(Level.FINE)) {
    log.fine("Associating ProcessInstance[" + processInstanceId + "] (@"+ scopedAssociation.getClass().getAnnotations()[0].annotationType().getSimpleName()+ ")");
  }
  scopedAssociation.setProcessInstanceId(processInstanceId);
}
