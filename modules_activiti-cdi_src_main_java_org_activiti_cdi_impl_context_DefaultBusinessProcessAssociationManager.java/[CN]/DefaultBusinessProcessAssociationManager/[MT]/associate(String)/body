{
  ScopedAssociation scopedAssociation=getScopedAssociation();
  if (log.isLoggable(Level.FINE)) {
    log.fine("Associating Execution[" + executionId + "] (@"+ scopedAssociation.getClass().getAnnotations()[0].annotationType().getSimpleName()+ ")");
  }
  scopedAssociation.setExecutionId(executionId);
}
