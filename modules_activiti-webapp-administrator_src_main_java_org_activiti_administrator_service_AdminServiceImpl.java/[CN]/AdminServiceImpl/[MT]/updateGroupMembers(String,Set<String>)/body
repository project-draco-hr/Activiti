{
  Set<String> members=getGroupMembers(id);
  for (  String user : members) {
    if (!users.contains(user))     getIdentityService().deleteMembership(user,id);
  }
  members=getGroupMembers(id);
  for (  String user : users) {
    if (!members.contains(user))     getIdentityService().createMembership(user,id);
  }
}
