{
  this.type=type;
switch (type) {
case GROUPS:
    this.candidateUsers=null;
  this.assignee=null;
break;
case INITIATOR:
this.candidateGroups=null;
this.candidateUsers=null;
this.assignee=null;
break;
case MIXED:
this.assignee=null;
break;
case USER:
this.candidateGroups=null;
this.candidateUsers=null;
break;
case USERS:
this.candidateGroups=null;
this.assignee=null;
}
}
