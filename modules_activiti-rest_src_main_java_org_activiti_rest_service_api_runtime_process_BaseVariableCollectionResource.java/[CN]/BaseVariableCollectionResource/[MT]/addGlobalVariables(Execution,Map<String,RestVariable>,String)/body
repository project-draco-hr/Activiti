{
  Map<String,Object> rawVariables=runtimeService.getVariables(execution.getId());
  List<RestVariable> globalVariables=restResponseFactory.createRestVariables(rawVariables,execution.getId(),RestResponseFactory.VARIABLE_EXECUTION,RestVariableScope.GLOBAL,serverRootUrl);
  for (  RestVariable var : globalVariables) {
    if (!variableMap.containsKey(var.getName())) {
      variableMap.put(var.getName(),var);
    }
  }
}
