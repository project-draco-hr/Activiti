{
  Content content=super.getContent(artifact);
  try {
    content=CycleServiceFactory.getContentService().transformContent(content,CycleApplicationContext.get(TextMimeType.class),CycleApplicationContext.get(XmlMimeType.class));
  }
 catch (  Exception e) {
    log.log(Level.WARNING,"error while applying XML-Content transformation: " + e.getMessage(),e);
  }
  return content;
}
