{
  processEngineConfiguration.setDeploymentName(defaultText(activitiProperties.getDeploymentName(),processEngineConfiguration.getDeploymentName()));
  processEngineConfiguration.setDatabaseSchema(defaultText(activitiProperties.getDatabaseSchema(),processEngineConfiguration.getDatabaseSchema()));
  processEngineConfiguration.setDatabaseSchema(defaultText(activitiProperties.getDatabaseSchemaUpdate(),processEngineConfiguration.getDatabaseSchemaUpdate()));
}
