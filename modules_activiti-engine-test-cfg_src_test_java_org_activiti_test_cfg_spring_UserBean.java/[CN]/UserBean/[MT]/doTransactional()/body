{
  processEngine.getRepositoryService().createDeployment().addString("userprocess.bpmn20.xml","<definitions " + NAMESPACE + " "+ TARGET_NAMESPACE+ ">"+ "  <process id='IDR' name='Insurance Damage Report' />"+ "</definitions>").deploy();
  if (fail) {
    processEngine.getRepositoryService().createDeployment().addString("invalidprocess.bpmn20.xml","<definitions " + NAMESPACE + " "+ TARGET_NAMESPACE+ ">"+ "  <aprocess id='IDR' name='Insurance Damage Report' />"+ "</definitions>").deploy();
  }
}
