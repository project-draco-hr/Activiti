{
  commandContext.getEventDispatcher().dispatchEvent(ActivitiEventBuilder.createActivityEvent(ActivitiEventType.ACTIVITY_TIMEOUT,activity.getId(),(String)activity.getProperties().get("name"),execution.getId(),execution.getProcessInstanceId(),execution.getProcessDefinitionId(),(String)activity.getProperties().get("type"),activity.getActivityBehavior().getClass().getCanonicalName()));
}
