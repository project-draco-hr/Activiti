{
  if (authenticate() == false)   return null;
  String userId=(String)getRequest().getAttributes().get("userId");
  if (userId == null) {
    throw new ActivitiException("No userId provided");
  }
  DataResponse dataResponse=new DefaultPaginateList().paginateList(getQuery(),ActivitiUtil.getIdentityService().createGroupQuery().groupMember(userId),"id",properties);
  return dataResponse;
}
