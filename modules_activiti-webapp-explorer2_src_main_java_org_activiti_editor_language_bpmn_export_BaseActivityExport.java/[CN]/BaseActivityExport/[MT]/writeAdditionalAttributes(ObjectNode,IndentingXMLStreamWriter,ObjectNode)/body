{
  String asynchronous=getPropertyValueAsString(PROPERTY_ASYNCHRONOUS,objectNode);
  if (StringUtils.isNotEmpty(asynchronous) && "yes".equalsIgnoreCase(asynchronous)) {
    writeQualifiedAttribute("async","true",xtw);
  }
  String exclusive=getPropertyValueAsString(PROPERTY_EXCLUSIVE,objectNode);
  if (StringUtils.isNotEmpty(exclusive) && "no".equalsIgnoreCase(asynchronous)) {
    writeQualifiedAttribute("exclusive","false",xtw);
  }
  super.writeAdditionalAttributes(objectNode,xtw,modelNode);
}
