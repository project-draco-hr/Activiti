{
  for (  Shape incomingShape : edge.getIncomings()) {
    if (shapeIsContainedIn(incomingShape,parent)) {
      for (      Shape outgoingShape : edge.getOutgoings()) {
        if (shapeIsContainedIn(outgoingShape,parent)) {
          return true;
        }
      }
      return false;
    }
  }
  return false;
}
