{
  if (cachedProcessEngine == null) {
    ProcessEngineConfigurationImpl processEngineConfiguration=(ProcessEngineConfigurationImpl)ProcessEngineConfiguration.createProcessEngineConfigurationFromResource("org/activiti/engine/test/bpmn/usertask/activiti.cfg.xml");
    cachedProcessEngine=processEngineConfiguration.buildProcessEngine();
  }
  processEngine=cachedProcessEngine;
}
