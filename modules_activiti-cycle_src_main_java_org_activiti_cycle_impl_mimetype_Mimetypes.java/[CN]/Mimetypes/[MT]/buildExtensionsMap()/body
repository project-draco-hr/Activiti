{
  if (extensionsMap == null) {
synchronized (this) {
      if (extensionsMap != null) {
        return;
      }
      extensionsMap=new HashMap<String,MimeType>();
      for (      MimeType type : getAvailableMimetypes()) {
        for (        String extension : type.getCommonFileExtensions()) {
          extensionsMap.put(extension,type);
        }
      }
    }
  }
}
