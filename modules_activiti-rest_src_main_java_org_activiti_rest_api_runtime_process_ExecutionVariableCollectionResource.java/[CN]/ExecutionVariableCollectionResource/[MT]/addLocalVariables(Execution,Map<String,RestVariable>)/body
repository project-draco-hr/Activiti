{
  Map<String,Object> rawLocalvariables=ActivitiUtil.getRuntimeService().getVariablesLocal(execution.getId());
  List<RestVariable> localVariables=getApplication(ActivitiRestServicesApplication.class).getRestResponseFactory().createRestVariables(this,rawLocalvariables,null,execution.getId(),null,RestVariableScope.LOCAL);
  for (  RestVariable var : localVariables) {
    variableMap.put(var.getName(),var);
  }
}
