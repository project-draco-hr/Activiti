{
  if ("executionId".equalsIgnoreCase(key))   return Context.getExecutionContext().getExecution().getId();
  if ("processInstance".equalsIgnoreCase(key))   return Context.getExecutionContext().getProcessInstance();
  if ("processInstanceId".equalsIgnoreCase(key))   return Context.getExecutionContext().getProcessInstance().getId();
  return null;
}
