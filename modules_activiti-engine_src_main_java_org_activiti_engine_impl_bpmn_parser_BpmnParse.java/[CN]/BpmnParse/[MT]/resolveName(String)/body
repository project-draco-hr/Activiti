{
  if (name == null) {
    return null;
  }
  int indexOfP=name.indexOf(':');
  if (indexOfP != -1) {
    String prefix=name.substring(0,indexOfP);
    String resolvedPrefix=this.prefixs.get(prefix);
    return resolvedPrefix + ":" + name.substring(indexOfP + 1);
  }
 else {
    return this.targetNamespace + ":" + name;
  }
}
