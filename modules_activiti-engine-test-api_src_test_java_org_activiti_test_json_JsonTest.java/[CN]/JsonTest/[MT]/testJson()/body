{
  deployer.deployProcessString(("<definitions xmlns='http://www.omg.org/spec/BPMN/20100524/MODEL'" + "             targetNamespace='http://www.activiti.org/bpmn2.0'>" + "  <process id='LoanProcess' >"+ "    <startEvent id='theStart' />"+ "    <sequenceFlow id='flow1' sourceRef='theStart' targetRef='theTask' />"+ "    <userTask id='theTask' name='my task' />"+ "    <sequenceFlow id='flow2' sourceRef='theTask' targetRef='theEnd' />"+ "    <endEvent id='theEnd' />"+ "  </process>"+ "</definitions>"));
  deployer.deployProcessString(("<definitions xmlns='http://www.omg.org/spec/BPMN/20100524/MODEL'" + "             targetNamespace='http://www.activiti.org/bpmn2.0'>" + "  <process id='ExpenseNoteProcess' >"+ "    <startEvent id='theStart' />"+ "    <sequenceFlow id='flow1' sourceRef='theStart' targetRef='theTask' />"+ "    <userTask id='theTask' name='my task' />"+ "    <sequenceFlow id='flow2' sourceRef='theTask' targetRef='theEnd' />"+ "    <endEvent id='theEnd' />"+ "  </process>"+ "</definitions>"));
  List<ProcessDefinition> processDefinitions=deployer.getRepositoryService().findProcessDefinitions();
  JsonListConverter<ProcessDefinition> jsonListConverter=new JsonListConverter<ProcessDefinition>(new JsonProcessDefinitionConverter());
  System.out.println(jsonListConverter.toJson(processDefinitions,2));
