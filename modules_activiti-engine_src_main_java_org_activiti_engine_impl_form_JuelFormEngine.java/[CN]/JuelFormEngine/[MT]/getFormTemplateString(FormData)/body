{
  String deploymentId=formInstance.getDeploymentId();
  String formKey=formInstance.getFormKey();
  ResourceEntity resourceStream=Context.getCommandContext().getRepositorySession().findResourceByDeploymentIdAndResourceName(deploymentId,formKey);
  byte[] resourceBytes=resourceStream.getBytes();
  String formTemplateString=new String(resourceBytes);
  return formTemplateString;
}
