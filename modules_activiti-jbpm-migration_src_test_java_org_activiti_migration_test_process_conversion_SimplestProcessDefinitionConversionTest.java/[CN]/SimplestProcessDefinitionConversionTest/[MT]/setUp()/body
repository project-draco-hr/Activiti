{
  super.setUp();
  JbpmContext jbpmContext=jbpmConfiguration.createJbpmContext();
  try {
    ProcessDefinition processDefinition=ProcessDefinition.parseXmlResource("org/activiti/migration/test/process/conversion/simplest/processdefinition.xml");
    jbpmContext.deployProcessDefinition(processDefinition);
    jbpmContext.newProcessInstance("simplest");
  }
  finally {
    jbpmContext.close();
  }
}
