{
  TaskQuery query=taskService.createTaskQuery().taskName("1");
  assertNull(query.singleResult());
  assertEquals(0,query.list().size());
  assertEquals(0,query.count());
  try {
    taskService.createTaskQuery().taskName(null).singleResult();
    fail();
