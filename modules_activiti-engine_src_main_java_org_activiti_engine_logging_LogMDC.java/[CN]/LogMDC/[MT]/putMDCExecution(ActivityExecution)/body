{
  if (e.getId() != null)   MDC.put(LOG_MDC_EXECUTION_ID,e.getId());
  if (e.getProcessDefinitionId() != null)   MDC.put(LOG_MDC_PROCESSDEFINITION_ID,e.getProcessDefinitionId());
  if (e.getProcessInstanceId() != null)   MDC.put(LOG_MDC_PROCESSINSTANCE_ID,e.getProcessInstanceId());
  if (e.getProcessBusinessKey() != null)   MDC.put(LOG_MDC_BUSINESS_KEY,e.getProcessBusinessKey());
}
