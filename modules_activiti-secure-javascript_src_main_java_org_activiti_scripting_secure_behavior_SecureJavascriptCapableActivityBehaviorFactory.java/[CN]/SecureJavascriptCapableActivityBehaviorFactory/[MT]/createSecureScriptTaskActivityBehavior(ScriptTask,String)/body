{
  SecureJavascriptTaskActivityBehavior secureScriptTaskActivityBehavior=new SecureJavascriptTaskActivityBehavior(scriptTask.getId(),scriptTask.getScript(),language,scriptTask.getResultVariable(),scriptTask.isAutoStoreVariables());
  if (SECURE_SCRIPT_CONTEXT_FACTORY == null) {
    initSecureScriptContextFactory();
  }
  return secureScriptTaskActivityBehavior;
}
