{
  if (migratedProcessDefinitions == null || migratedProcessDefinitions.size() == 0) {
    convertProcesses();
  }
  ProcessEngine processEngine=createProcessEngine();
  for (  String processName : migratedProcessDefinitions.keySet()) {
    processEngine.getRepositoryService().createDeployment().addString(processName.replace(" ","_") + ".bpmn20.xml",XmlUtil.toString(migratedProcessDefinitions.get(processName))).deploy();
  }
  processEngine.close();
}
