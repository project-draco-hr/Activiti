{
  if (isProcessInstance()) {
    text.append("ProcInst");
  }
 else {
    text.append("Exe");
  }
  if (activity != null) {
    text.append("(");
    text.append(activity.getId());
    text.append(")");
  }
  if (hasExecutions()) {
    text.append("[");
    for (    ExecutionImpl child : executions) {
      child.toString(text);
    }
    text.append("]");
  }
  return text.toString();
}
