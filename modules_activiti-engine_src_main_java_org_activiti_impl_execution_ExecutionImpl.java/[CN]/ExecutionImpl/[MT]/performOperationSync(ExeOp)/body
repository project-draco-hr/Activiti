{
  this.nextOperation=executionOperation;
  if (!isOperating) {
    isOperating=true;
    while (nextOperation != null) {
      ExeOp currentOperation=this.nextOperation;
      this.nextOperation=null;
      log.fine("ExeOp: " + currentOperation + " on "+ this);
      currentOperation.execute(this);
    }
    isOperating=false;
  }
}
