{
  boolean found=false;
  for (  IdentityLink identityLink : links) {
    if (userId != null && userId.equals(identityLink.getUserId())) {
      found=true;
      break;
    }
 else     if (groupId != null && groupId.equals(identityLink.getGroupId())) {
      found=true;
      break;
    }
  }
  return found;
}
