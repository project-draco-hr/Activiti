{
  StringBuilder s=null;
  s=new StringBuilder();
  parseNode("${1+1}").appendStructure(s,bindings);
  parseNode("${1*1}").appendStructure(s,bindings);
  parseNode("${1/1}").appendStructure(s,bindings);
  parseNode("${1%1}").appendStructure(s,bindings);
  assertEquals("1 + 11 * 11 / 11 % 1",s.toString());
  s=new StringBuilder();
  parseNode("${1<1}").appendStructure(s,bindings);
  parseNode("${1>1}").appendStructure(s,bindings);
  parseNode("${1<=1}").appendStructure(s,bindings);
  parseNode("${1>=1}").appendStructure(s,bindings);
  assertEquals("1 < 11 > 11 <= 11 >= 1",s.toString());
  s=new StringBuilder();
  parseNode("${1==1}").appendStructure(s,bindings);
  parseNode("${1!=1}").appendStructure(s,bindings);
  assertEquals("1 == 11 != 1",s.toString());
  s=new StringBuilder();
  parseNode("${1&&1}").appendStructure(s,bindings);
  parseNode("${1||1}").appendStructure(s,bindings);
  assertEquals("1 && 11 || 1",s.toString());
}
