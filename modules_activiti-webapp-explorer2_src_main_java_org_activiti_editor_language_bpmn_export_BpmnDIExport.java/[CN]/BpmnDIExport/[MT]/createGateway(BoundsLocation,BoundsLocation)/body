{
  double middleX=upperLeft.x + ((lowerRight.x - upperLeft.x) / 2);
  double middleY=upperLeft.y + ((lowerRight.y - upperLeft.y) / 2);
  Polyline2D gatewayRectangle=new Polyline2D(new Point2D(upperLeft.x,middleY),new Point2D(middleX,upperLeft.y),new Point2D(lowerRight.x,middleY),new Point2D(middleX,lowerRight.y),new Point2D(upperLeft.x,middleY));
  return gatewayRectangle;
}
