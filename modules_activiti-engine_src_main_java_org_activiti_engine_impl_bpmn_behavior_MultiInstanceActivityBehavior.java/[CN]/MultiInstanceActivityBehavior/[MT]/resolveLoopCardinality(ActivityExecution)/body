{
  Object value=loopCardinalityExpression.getValue(execution);
  if (value instanceof Number) {
    return ((Number)value).intValue();
  }
 else   if (value instanceof String) {
    return Integer.valueOf((String)value);
  }
 else {
    throw new ActivitiException("Could not resolve loopCardinality expression '" + loopCardinalityExpression.getExpressionText() + "': not a number nor number String");
  }
}
