{
  FileInputStream is=null;
  try {
    is=new FileInputStream(new File(path));
    Properties properties=new Properties();
    properties.load(is);
    return properties;
  }
 catch (  IOException e) {
    throw new ActivitiException("Couldn't read " + path,e);
  }
 finally {
    IoUtil.closeSilently(is);
  }
}
