{
  EntityManager entityManager=createEntityManager();
  T entity=null;
  try {
    entity=entityManager.find(entityClass,id);
  }
 catch (  Exception e) {
    LOGGER.log(Level.SEVERE,"Error getting object " + entityClass + " by id "+ id,e);
  }
 finally {
    entityManager.close();
  }
  return entity;
}
