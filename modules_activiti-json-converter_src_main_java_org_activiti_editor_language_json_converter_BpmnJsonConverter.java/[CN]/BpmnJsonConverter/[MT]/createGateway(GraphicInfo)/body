{
  double middleX=graphicInfo.x + (graphicInfo.width / 2);
  double middleY=graphicInfo.y + (graphicInfo.height / 2);
  Polyline2D gatewayRectangle=new Polyline2D(new Point2D(graphicInfo.x,middleY),new Point2D(middleX,graphicInfo.y),new Point2D(graphicInfo.x + graphicInfo.width,middleY),new Point2D(middleX,graphicInfo.y + graphicInfo.height),new Point2D(graphicInfo.x,middleY));
  return gatewayRectangle;
}
