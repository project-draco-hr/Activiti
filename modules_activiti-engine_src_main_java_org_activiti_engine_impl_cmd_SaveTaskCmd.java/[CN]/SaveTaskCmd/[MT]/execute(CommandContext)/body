{
  if (task == null) {
    throw new ActivitiException("task is null");
  }
  if (task.getRevision() == 0) {
    task.insert(null);
  }
 else {
    TaskEntity persistentTask=Context.getCommandContext().getTaskManager().findTaskById(task.getId());
    persistentTask.update(task);
  }
  return null;
}
