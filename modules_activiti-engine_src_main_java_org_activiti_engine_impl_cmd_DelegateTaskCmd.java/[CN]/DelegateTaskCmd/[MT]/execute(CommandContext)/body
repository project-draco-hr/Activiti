{
  if (taskId == null) {
    throw new ActivitiException("taskId is null");
  }
  TaskEntity task=Context.getCommandContext().getTaskManager().findTaskById(taskId);
  if (task == null) {
    throw new ActivitiException("Cannot find task with id " + taskId);
  }
  task.delegate(userId);
  return null;
}
