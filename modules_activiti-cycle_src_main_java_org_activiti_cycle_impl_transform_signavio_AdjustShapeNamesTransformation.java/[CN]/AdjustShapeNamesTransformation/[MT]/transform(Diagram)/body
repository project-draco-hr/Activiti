{
  for (  Shape shape : diagram.getShapes()) {
    if (shape.getProperties().containsKey("name")) {
      String name=shape.getProperty("name");
      String newName=name.replaceAll("\n"," ").replaceAll("'","").replaceAll("\"","");
      shape.getProperties().put("name",newName);
    }
  }
  return diagram;
}
