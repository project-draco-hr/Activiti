{
  if (base == null) {
    String variable=(String)property;
    if (EXECUTION_KEY.equals(property)) {
      context.setPropertyResolved(true);
      return execution;
    }
 else {
      if (execution.hasVariable(variable)) {
        context.setPropertyResolved(true);
        return execution.getVariable(variable);
      }
    }
  }
  return null;
}
