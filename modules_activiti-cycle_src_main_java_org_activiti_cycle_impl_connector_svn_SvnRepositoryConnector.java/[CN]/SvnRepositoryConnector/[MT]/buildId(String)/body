{
  String result="";
  for (int i=0; i < constituents.length; i++) {
    String constituent=constituents[i];
    if (i == 0) {
      result=constituents[i];
      continue;
    }
    if (!result.endsWith("/") && !constituent.startsWith("/") && constituent.length() > 0) {
      result+="/";
    }
    result+=constituent;
  }
  return result;
}
