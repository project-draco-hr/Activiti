{
  if (resources == null && id != null) {
    List<ResourceEntity> resourcesList=Context.getCommandContext().getSession(RepositorySession.class).findResourcesByDeploymentId(id);
    resources=new HashMap<String,ResourceEntity>();
    for (    ResourceEntity resource : resourcesList) {
      resources.put(resource.getName(),resource);
    }
  }
  return resources;
}
