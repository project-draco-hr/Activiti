        }
      }
 else {
        processDiagramCanvas.drawThrowingNoneEvent((int)graphicInfo.getX(),(int)graphicInfo.getY(),(int)graphicInfo.getWidth(),(int)graphicInfo.getHeight());
      }
    }
  }
