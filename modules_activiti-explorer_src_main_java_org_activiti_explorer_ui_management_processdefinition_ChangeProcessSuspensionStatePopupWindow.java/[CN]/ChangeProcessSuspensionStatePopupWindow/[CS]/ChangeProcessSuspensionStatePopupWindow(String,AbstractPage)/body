  setWidth(400,UNITS_PIXELS);
  setHeight(300,UNITS_PIXELS);
  addStyleName(Reindeer.WINDOW_LIGHT);
  verticalLayout=new VerticalLayout();
  addComponent(verticalLayout);
  addTimeSection();
  addIncludeProcessInstancesSection();
  addOkButton();
}
