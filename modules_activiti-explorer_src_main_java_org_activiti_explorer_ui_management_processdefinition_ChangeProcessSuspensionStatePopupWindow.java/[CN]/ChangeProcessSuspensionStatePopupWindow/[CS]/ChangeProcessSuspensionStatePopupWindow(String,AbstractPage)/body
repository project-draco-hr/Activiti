{
  this.processDefinitionId=processDefinitionId;
  this.parentPage=parentPage;
  this.i18nManager=ExplorerApp.get().getI18nManager();
  setCaption(i18nManager.getMessage(Messages.PROCESS_SUSPEND_POPUP));
  setModal(true);
  center();
