{
  String varName=null;
  for (int i=0; i < 5; i++) {
    varName="variable-" + new Random().nextInt(10);
    delegateTask.getExecution().setVariable(varName,getRandomValue());
  }
  for (int i=0; i < 5; i++) {
    varName="task-variable-" + new Random().nextInt(10);
    delegateTask.setVariableLocal(varName,getRandomValue());
  }
}
