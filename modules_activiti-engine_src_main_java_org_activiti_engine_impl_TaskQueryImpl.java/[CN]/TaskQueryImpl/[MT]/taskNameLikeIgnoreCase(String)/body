{
  if (nameLikeIgnoreCase == null) {
    throw new ActivitiIllegalArgumentException("Task nameLikeIgnoreCase is null");
  }
  if (orActive) {
    orQueryObject.nameLikeIgnoreCase=nameLikeIgnoreCase.toLowerCase();
