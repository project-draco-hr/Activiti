{
  if (orActive) {
    if (delegationState == null) {
      currentOrQueryObject.noDelegationState=true;
    }
 else {
      currentOrQueryObject.delegationState=delegationState;
    }
  }
 else {
    if (delegationState == null) {
      this.noDelegationState=true;
    }
 else {
      this.delegationState=delegationState;
    }
  }
  return this;
}
