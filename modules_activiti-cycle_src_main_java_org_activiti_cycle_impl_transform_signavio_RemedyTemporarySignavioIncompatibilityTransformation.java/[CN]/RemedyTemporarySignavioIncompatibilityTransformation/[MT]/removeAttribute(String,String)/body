{
  int startIndex=xml.indexOf(" " + attributeName + "=\"");
  while (startIndex != -1) {
    int endIndex=xml.indexOf("\"",startIndex + attributeName.length() + 3);
    xml=xml.substring(0,startIndex) + xml.substring(endIndex + 1);
    startIndex=xml.indexOf(" " + attributeName + "=\"");
  }
  return xml;
}
