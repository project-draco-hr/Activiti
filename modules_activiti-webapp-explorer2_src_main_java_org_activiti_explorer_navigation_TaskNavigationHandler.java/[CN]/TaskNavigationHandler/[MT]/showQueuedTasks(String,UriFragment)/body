{
  String groupId=uriFragment.getParameter(PARAMETER_GROUP);
  if (groupId != null) {
    ExplorerApp.get().getViewManager().showTaskQueuedPage(groupId,taskId);
  }
 else {
    showInbox(taskId,uriFragment);
    ExplorerApp.get().getNotificationManager().showErrorNotification("Cannot view queued tasks","No groupId was provided, can't show queued tasks");
  }
}
