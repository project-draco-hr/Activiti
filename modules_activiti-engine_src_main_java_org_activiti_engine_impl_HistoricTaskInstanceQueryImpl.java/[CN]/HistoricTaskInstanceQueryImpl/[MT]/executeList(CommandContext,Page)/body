{
  ensureVariablesInitialized();
  checkQueryOk();
  if (includeTaskLocalVariables || includeProcessVariables) {
    return commandContext.getHistoricTaskInstanceEntityManager().findHistoricTaskInstancesAndVariablesByQueryCriteria(this,page);
  }
 else {
    return commandContext.getHistoricTaskInstanceEntityManager().findHistoricTaskInstancesByQueryCriteria(this,page);
  }
}
