{
  if (user == null) {
    throw new ActivitiException("user is null");
  }
  if (user.getRevision() == 0) {
    commandContext.getIdentitySession().insertUser(user);
  }
 else {
    UserEntity persistentUser=commandContext.getIdentitySession().findUserById(user.getId());
    persistentUser.update(user);
  }
  return null;
}
