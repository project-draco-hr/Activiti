{
  if (isValidEvent(event)) {
    if (event.getType() == ActivitiEventType.ENTITY_CREATED) {
      onCreate(event);
    }
    if (event.getType() == ActivitiEventType.ENTITY_CREATED) {
      onInitialized(event);
    }
 else     if (event.getType() == ActivitiEventType.ENTITY_DELETED) {
      onDelete(event);
    }
 else     if (event.getType() == ActivitiEventType.ENTITY_UPDATED) {
      onUpdate(event);
    }
 else {
      onEntityEvent(event);
    }
  }
}
