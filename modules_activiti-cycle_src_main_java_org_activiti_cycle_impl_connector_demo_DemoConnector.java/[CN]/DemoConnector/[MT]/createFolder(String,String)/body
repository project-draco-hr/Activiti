{
  String id=parentFolderId;
  if (!id.startsWith("/")) {
    id="/" + id;
  }
  if (id.length() > 1) {
    id=id + "/";
  }
  id=id + name;
  RepositoryFolderImpl newFolder=new RepositoryFolderImpl(getConfiguration().getId(),id);
  newFolder.getMetadata().setName(name);
  newFolder.getMetadata().setParentFolderId(parentFolderId);
  nodes.add(newFolder);
  return newFolder;
}
