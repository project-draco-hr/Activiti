{
  SqlSessionFactory sqlMapper=getIbatisConfig();
  SqlSession session=sqlMapper.openSession();
  CycleConfigEntity cycleConfig=null;
  try {
    cycleConfig=(CycleConfigEntity)session.selectOne("org.activiti.cycle.impl.conf.CycleConfigEntity.selectCycleConfigById",id);
  }
  finally {
    session.close();
  }
  return cycleConfig;
}
