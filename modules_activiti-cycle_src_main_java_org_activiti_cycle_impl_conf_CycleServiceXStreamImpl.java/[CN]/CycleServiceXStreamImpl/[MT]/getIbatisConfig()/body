{
  Reader reader=null;
  String resource="org/activiti/cycle/impl/db/ibatis/activiti.ibatis.mem.conf.xml";
  try {
    reader=Resources.getResourceAsReader(resource);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  SqlSessionFactory sqlMapper=new SqlSessionFactoryBuilder().build(reader);
  return sqlMapper;
}
