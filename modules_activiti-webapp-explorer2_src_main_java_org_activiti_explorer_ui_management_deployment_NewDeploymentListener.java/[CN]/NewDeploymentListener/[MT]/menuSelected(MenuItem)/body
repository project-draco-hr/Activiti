{
  DeploymentUploadReceiver receiver=new DeploymentUploadReceiver();
  UploadPopupWindow uploadPopupWindow=new UploadPopupWindow(ExplorerApplication.getCurrent().getMessage(Messages.DEPLOYMENT_UPLOAD),ExplorerApplication.getCurrent().getMessage(Messages.DEPLOYMENT_UPLOAD_DESCRIPTION),receiver);
  uploadPopupWindow.addFinishedListener(receiver);
  ExplorerApplication.getCurrent().showPopupWindow(uploadPopupWindow);
}
