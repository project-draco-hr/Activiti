{
  Deployment deployment=repositoryService.createDeployment().addString("org/activiti/test/HelloWorld.string","hello world").addString("org/activiti/test/TheAnswer.string","42").deploy();
  List<Deployment> deployments=repositoryService.findDeployments();
  assertEquals(1,deployments.size());
  repositoryService.deleteDeploymentCascade(deployment.getId());
}
