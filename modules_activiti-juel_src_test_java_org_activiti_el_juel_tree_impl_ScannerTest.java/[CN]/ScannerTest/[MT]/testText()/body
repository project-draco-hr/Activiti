{
  Symbol[] expected={TEXT};
  assertEquals(expected,symbols("foo"));
  assertEquals(expected,symbols("foo\\"));
  assertEquals(expected,symbols("foo\\$"));
  assertEquals(expected,symbols("foo\\#"));
  assertEquals(expected,symbols("foo\\${"));
  assertEquals(expected,symbols("foo\\#{"));
  assertEquals(expected,symbols("\\${foo}"));
  assertEquals(expected,symbols("\\${foo}"));
}
