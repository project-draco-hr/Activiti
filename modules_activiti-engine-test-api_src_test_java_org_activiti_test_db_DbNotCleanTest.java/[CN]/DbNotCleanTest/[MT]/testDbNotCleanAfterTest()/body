{
  Deployment deployment=repositoryService.createDeployment().addString("test.bpmn20.xml","<definitions xmlns='http://www.omg.org/spec/BPMN/20100524/MODEL' " + "targetNamespace='http://www.activiti.org/bpmn2.0' />").deploy();
  assertNotNull(deployment);
  try {
    assertAndEnsureCleanDb();
    fail();
  }
 catch (  ActivitiException e) {
    assertTextPresent("Database not clean",e.getMessage());
  }
 finally {
    repositoryService.deleteDeployment(deployment.getId());
  }
}
