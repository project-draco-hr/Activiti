{
  Element jobExecutorElement=rootElement.element("job-executor");
  if (jobExecutorElement != null) {
    String autoActivateString=jobExecutorElement.attribute("auto-activate");
    if (autoActivateString != null) {
      if (autoActivateString.equals("off") || autoActivateString.equals("disabled") || autoActivateString.equals("false")) {
        this.isJobExecutorAutoActivate=false;
      }
 else       if (autoActivateString.equals("on") || autoActivateString.equals("enabled") || autoActivateString.equals("true")) {
        this.isJobExecutorAutoActivate=true;
      }
 else {
        addError("Invalid value for 'auto-activate', current values are supported:" + "on/off enabled/disabled true/false",jobExecutorElement);
      }
    }
  }
}
