{
  Element jobExecutorElement=rootElement.element("job-executor");
  if (jobExecutorElement != null) {
    String activateString=jobExecutorElement.attribute("activate");
    if (activateString != null) {
      if (activateString.equals("off") || activateString.equals("disabled") || activateString.equals("false")) {
        this.jobExecutorActivate=false;
      }
 else       if (activateString.equals("on") || activateString.equals("enabled") || activateString.equals("true")) {
        this.jobExecutorActivate=true;
      }
 else {
        addError("Invalid value for 'auto-activate', current values are supported:" + "on/off enabled/disabled true/false",jobExecutorElement);
      }
    }
  }
}
