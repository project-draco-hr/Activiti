{
  if (group == null) {
    throw new ActivitiException("group is null");
  }
  if (group.getRevision() == 0) {
    commandContext.getIdentitySession().insertGroup(group);
  }
 else {
    GroupEntity persistentGroup=commandContext.getIdentitySession().findGroupById(group.getId());
    persistentGroup.update(group);
  }
  return null;
}
